///Print the number Scala while loop Example.

import scala.collection.mutable
import scala.io.StdIn.{readChar, readInt, readLine}

object Demo {
  def main(args: Array[String]): Unit = {

  println("Enter the value");
  var Value= readInt();
   while (Value < 10) {
     Value = Value + 1;
     println(Value);
   }
